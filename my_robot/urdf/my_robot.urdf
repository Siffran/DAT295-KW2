<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="my_robot">
  <xacro:property name="length" value="2" />
  <xacro:property name="width" value="1" />
  <xacro:property name="height" value="0.5" />
  <link name="base_link">
    <visual>
      <geometry>
        <box size="${length} ${width} ${height}"/>
      </geometry>
    </visual>
  </link>
</robot>
